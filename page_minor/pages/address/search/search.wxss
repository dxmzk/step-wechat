.search-box {
  height: 88rpx;
  min-height: 88rpx;
  padding-left: 32rpx;
  align-items: center;
  background-color: white;
}
.icon {
  width: 28rpx;
  height: 28rpx;
  margin: 0 8rpx;
}
.history {
  padding: 0 32rpx;
  background-color: white;
}
.label-title {
  color: #232323;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  font-weight: 500;
  line-height: 96rpx;
}
.clear-ic {
  width: 38rpx;
  height: 38rpx;
}

.input {
  background: #f2f2f2;
  border-radius: 8rpx;
  height: 64rpx;
  padding: 0 16rpx;
  margin-left: 16rpx;
  align-items: center;
}
.search-btn {
  padding: 16rpx 24rpx 16rpx 32rpx;
}
.wrap {
  flex-wrap: wrap;
  min-height: 72rpx;
  border-bottom: solid 1px #f2f2f2;
}
.node {
  padding: 8rpx 24rpx;
  margin-left: 16rpx;
  margin-bottom: 24rpx;
  background-color: #F5F5F5;
  border-radius: 8rpx;
}
.list {
  background-color: white;
}
.item {
  display: flex;
  margin-left: 32rpx;
  padding: 24rpx 32rpx 24rpx 0;
  justify-content: center;
  flex-direction: column;
  border-bottom: solid 1px #f5f5f5;
}
.address-name {
  font-size: 30rpx;
  color: #323232;
}
.address-info {
  font-size: 24rpx;
  color: #979797;
  margin-top: 8rpx;
}