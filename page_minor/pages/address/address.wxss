.address {
  flex: 1;
  background-color: #f5f5f5;
}
.list {

}
.item-box {
  min-height: 164rpx;
  padding: 16rpx 32rpx;
  align-items: center;
  background-color: white;
}
.item-def-str {
  background-color: #fef0e5;
  border-radius: 6rpx;
  margin-right: 16rpx;
  color: #f56105;
  font-size: 20rpx;
  padding: 3rpx 8rpx;
  line-height: 22rpx;
}
.def-str2 {
  background-color: #e5faef;
  color: #05c25d;
}
.user-str {
  color: #232323;
  font-size: 32rpx;
  font-weight: 500;
  line-height: 50rpx;
  font-family: PingFangSC-Medium;
}
.phone {
  color: #979797;
  font-size: 28rpx;
  margin-left: 16rpx;
}
.ads-str {
  color: #676767;
  font-size: 28rpx;
  margin-right: 32rpx;
}
.ads-yoe {
  color: #ff7100;
  font-size: 28rpx;
  margin-right: 32rpx;
}
.not-use-box {
  height: 80rpx;
  min-height: 80rpx;
  display: flex;
  align-items: center;
  margin-left: 32rpx;
  margin-top: 12rpx;

  font-size: 24rpx;
  color: #676767;
}
.edit-ic {
  width: 48rpx;
  height: 48rpx;
}
.alert-ic {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.footer-action {
  z-index: 1;
  width: 100%;
  bottom: 0;
  position: fixed;
  background-color: white;
}
.footer-btn {
  z-index: 1;
  color: white;
  font-size: 36rpx;
  font-weight: 500;
  font-family: PingFangSC-Medium;

  flex: 1;
  height: 88rpx;
  line-height: 88rpx;
  margin: 12rpx 32rpx;
  text-align: center;
  border-radius: 8rpx;
  background-color: #ff7100;
}
