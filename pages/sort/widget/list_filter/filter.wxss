.v-list-filter {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  background-color: white;
}
.v-filter-view {
  flex: 1;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.v-filter-view-name {
  color: #363636;
  font-size: 28rpx;
}
.v-filter-on {
  color: #FE7100;
  font-size: 28rpx;
}
.v-filter-ic {
  width: 24rpx;
  height: 24rpx;
  margin-left: 6rpx;
}